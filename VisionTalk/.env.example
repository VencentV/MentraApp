# Server port - The port on which the application will run (default: 3000)
PORT=3000

# App package name - Your VisionTalk application's unique identifier from MentraOS Developer Console
PACKAGE_NAME=com.visiontalk.assistant

# MentraOS API key - Your developer API key from the MentraOS Developer Console
MENTRAOS_API_KEY=your_mentraos_api_key_here

# OpenAI API key - For GPT-4V vision analysis and reasoning
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API key - For natural voice synthesis  
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Public URL for ngrok or production deployment
PUBLIC_URL=https://your-ngrok-url.ngrok.app